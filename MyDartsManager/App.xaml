                <ResourceDictionary Source="UserInterface/Styles/ComboBox.xaml"/>
                <ResourceDictionary Source="UserInterface/Styles/DataGrid.xaml"/>
                <ResourceDictionary Source="UserInterface/Styles/CheckBox.xaml"/>
                <ResourceDictionary Source="UserInterface/Styles/Label.xaml"/>
                <ResourceDictionary Source="UserInterface/Styles/ListBox.xaml"/>
                <ResourceDictionary Source="UserInterface/Styles/TextBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
