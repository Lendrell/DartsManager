        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyDartsManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="1440" Width="2560">
    <Grid Background="{StaticResource BackgroundColor}">
        <Button Style="{StaticResource DartsButtonStyle}" Name="welcomeBtn" Content="Welcome" FontSize="80" HorizontalAlignment="Center" VerticalAlignment="Center" Click="welcomeBtnOnClick"/>

    </Grid>
</Window>
